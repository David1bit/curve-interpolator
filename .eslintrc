{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
    "one-var": "off",
    "no-mixed-operators": "off",
    "no-restricted-properties": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "one-var-declaration-per-line": "off",
    "import/prefer-default-export": "off"
  }
}
